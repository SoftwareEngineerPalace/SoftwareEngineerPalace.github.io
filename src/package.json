{
  "name": "my-app",
  "version": "1.0.0",
  "description": "My Vite app",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build && yarn delete && npm run mac_copy",
    "delete": "yarn delete_assets && yarn delete_index && yarn delete_qrcode",
    "delete_assets": "if [ -d ../assets ] ; then rm -rf ../assets ; fi ",
    "delete_index": "if [ -e ../index.html ] ; then rm ../index.html ; fi ",
    "delete_qrcode": "if [ -e ../qrcode.min.js ] ; then rm ../qrcode.min.js ; fi",
    "serve": "vite preview",
    "copy": "xcopy /s /i /y dist ../",
    "mac_copy": "cp -R dist/ ../."
  },
  "dependencies": {
    "ant-design-vue": "^3.2.17",
    "dayjs": "^1.11.7",
    "esbuild": "^0.17.15",
    "postcss": "^8.4.21",
    "query-string": "^8.1.0",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.47",
    "vue-router": "4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^1.4.4",
    "less": "^4.1.3",
    "vite": "^2.9.15"
  }
}
